{
  "name": "ember-material-design-icons-shim",
  "version": "0.0.0-development",
  "description": "A shim for Google Material Design Icons in Ember.js apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember try:each",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "repository": "https://github.com/mike-north/ember-material-design-icons-shim",
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "broccoli-asset-rev": "2.7.0",
    "ember-cli": "3.5.1",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-dependency-checker": "2.2.1",
    "ember-cli-eslint": "5.0.0",
    "ember-cli-htmlbars": "2.0.5",
    "ember-cli-htmlbars-inline-precompile": "1.0.5",
    "ember-cli-inject-live-reload": "1.10.2",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-shims": "1.2.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-test-loader": "^2.1.0",
    "ember-cli-uglify": "2.1.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "2.0.0",
    "ember-resolver": "4.5.6",
    "ember-source": "2.18.2",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.1.0",
    "eslint-plugin-ember": "5.4.0",
    "eslint-plugin-node": "8.0.0",
    "husky": "1.1.2",
    "loader.js": "4.7.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "ember-addon",
    "material-design",
    "icons"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.6.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  },
  "optionalDependencies": {
    "@mike-north/js-lib-renovate-config": "^1.0.0",
    "@mike-north/js-lib-semantic-release-config": "^1.0.1",
    "semantic-release": "^15.10.7",
    "travis-deploy-once": "^5.0.9"
  }
}
